{
  "name": "classroom-electron",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "package": "electron-packager ./teacher/ classroomTeacher --platform=win32 --win32metadata.ProductName 威爱通教室  --win32metadata.FileDescription 威爱通教师端  --out ./package --version 1.3.5 --overwrite --icon img/classroom.ico",
    "start": "concurrently \"npm run start-server\" \"npm run start-electron\"",
    "start-electron": "electron .",
    "start-server": "npm run start-server:dev",
    "start-server:dev": "cross-env NODE_ENV=development node server/index.js",
    "start-server:prod": "cross-env NODE_ENV=production node server/index.js",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf ./dist/*",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --color -p",
    "postbuild": "npm run package",
    "generate": "node ./generator/index.js"
  },
