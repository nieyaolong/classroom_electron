{
  "name": "classroom-electron",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-server": "electron ./server/server.js",
    "start-client": "electron ./client/client.js",
    "package-server": "electron-packager ./server/ classroomServer --platform=win32 --out ./package --version 1.3.5 --overwrite --icon img/classroom.ico",
    "package-client": "electron-packager ./client/ classroomClient --platform=win32 --out ./package --version 1.3.5 --overwrite --icon img/classroom.ico",
    "package-course": "electron-packager ./course/ classroomCourse --platform=win32 --out ./package --version 1.3.5 --overwrite --icon img/course.ico"
  },
  "dependencies": {
    "electron-config": "^0.2.1",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  }
}
