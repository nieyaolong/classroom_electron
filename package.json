{
  "name": "classroom-electron",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-server": "electron ./server/server.js",
    "start-client": "electron ./client/client.js",
    "package-server": "./server/ classroomServer --platform=mas --out ./package --version 1.3.5 --overwrite --icon client/classroom.icns",
    "package-client": "./client/ classroomClient --platform=mas --out ./package --version 1.3.5 --overwrite --icon client/classroom.icns",
    "package-course": "./course/ classroomCourse --platform=mas --out ./package --version 1.3.5 --overwrite --icon course/course.icns"
  },
  "dependencies": {
    "electron": "^1.3.5",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  }
}
